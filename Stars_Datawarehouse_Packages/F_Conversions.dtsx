              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT [Billing_ID]
      ,[Billing_Control_ID]
      ,[Billing_Status]
      ,[Claims_Attributed]
	  ,ISNULL(p.patient_ident,-99) AS patient_ident
	  ,c.Client_Ident
	  ,d.Date_Ident as Service_Date_Ident
	  ,[PDC_Bucket]
	  ,a.ADH_Status_Ident
      ,[StockInHand_Today]
	  ,[Min_PDC]
      ,[PDC_For_Month]
	  ,m.Measure_Ident
	  ,d2.Date_Ident as Create_datetime_ident
      ,[RatePerHead]
      ,[PMD_Invoice_No]
      ,ISNULL(u.user_ident,-99) AS user_ident
      ,b.Billing_Status_Ident
      ,pb.PDC_Bucket_Ident


 FROM [staging].[S_F_Conversions] s
  LEFT JOIN D_Patient p
  ON s.patient_unique_ident = p.patient_unique_ident
  INNER JOIN D_Client c
  ON s.pmd_client_id = c.PMD_Client_ID
  AND s.planidentifier_pclm = c.PlanIdentifier_PClm
  INNER JOIN D_Date d
  ON s.measurenotedate = CAST(d.date as DATE)
  INNER JOIN D_ADH_Status a
  ON CAST(s.measureadh_status_today AS VARCHAR(5)) = CAST(a.ADH_Status_Key AS VARCHAR(5))
  INNER JOIN D_Measure m
  ON s.measureuse = m.Measure_Use
  INNER JOIN D_Date d2
  ON CAST(s.create_datetime as DATE) = CAST(d2.date as DATE)  
  LEFT JOIN StarsConnect.dbo.calls cs
  ON cs.id = s.StarsConnect_Call_ID
  LEFT JOIN dbo.D_Users u
  ON u.[user_Id] = cs.[user_id]
  INNER JOIN dbo.D_Billing_Status b
  ON b.billing_status_dn = s.billing_status_dn  
  INNER JOIN dbo.D_PDC_Bucket pb
  ON pb.pdc_bucket_dn = s.PDC_Bucket</property>
