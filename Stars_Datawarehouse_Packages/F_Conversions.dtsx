      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{F130C9C1-8230-45DD-AA40-3603B0BFBE9E}"
          SQLTask:SqlStatementSource="--If the current date of the year is on or after 3/1 then import just the current year&#xA;&#xA;If GETDATE() &gt;= CAST('3/1/'+cast(Year(GETDATE()) as varchar(4)) as date) &#xA;&#xA;BEGIN&#xA;&#xA;&#xA;DELETE from dbo.F_Conversions where left(service_date_ident,4)= year(getdate())&#xA;&#xA;&#xA;END&#xA;&#xA;&#xA;&#xA;--IF the current date is before 3/1 then continue to import to previous year along with the current year &#xA; &#xA;IF GETDATE() &lt; CAST('3/1/'+cast(Year(GETDATE()) as varchar(4)) as date) &#xA;&#xA;BEGIN&#xA;&#xA;DELETE from dbo.F_Conversions where left(service_date_ident,4) &gt;= year(getdate())-1&#xA;&#xA;END" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
