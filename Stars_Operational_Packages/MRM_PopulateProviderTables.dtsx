                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT
            Provider_ID
          , First_Name
          , Middle_Name
          , Last_Name
          , Phone_Number
          , Fax_Number
          , cast(Provider_Type as varchar(10)) as Provider_Type
          , Organization_Name
          , [Address_1]
          , [Address_2]
          , City
          , State
          , Zip
          , DNC_Flag
        FROM
            Stars_Rdsm.dbo.Stars_Providers dr -- AND p.PMD_Provider_ID IS NOT NULL
        INNER JOIN Stars_Rdsm.dbo.Stars_Providers_Detail drd ON drd.Provider_Unique_Ident = dr.Provider_Unique_Ident
        WHERE
            drd.Phone_Number IS NOT NULL
            AND drd.Phone_Number &lt;&gt; ''''
            AND LEN(drd.Phone_Number) = 10
            AND drd.End_Date IS NULL
            AND dr.DNC_Flag = 0
            AND dr.Last_Name &lt;&gt; 'No Provider'
            AND dr.First_Name &lt;&gt; 'No Provider'
            AND ISNULL(dr.Last_Name , '') &lt;&gt; ''
            AND ISNULL(dr.First_Name , '') &lt;&gt; ''
            AND dr.First_Name &lt;&gt; 'NONE-DEA' AND dr.Provider_Source='RefNPI'</property>
