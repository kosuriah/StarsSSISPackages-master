                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT  DISTINCT --COUNT(DISTINCT dr.npi), CAST(c.created_at AS DATE)
    dr.npi--,ph.number
	--,c.call_type_id
FROM
    StarsConnect.dbo.calls c
INNER JOIN StarsConnect.dbo.providers dr ON c.entity_id = dr.id
INNER JOIN StarsConnect.dbo.status_details sd ON c.status_detail_id = sd.id
                                    AND sd.description = 'Wrong Number'
INNER JOIN StarsConnect.dbo.statuses s ON c.status_id = s.id
                                          AND s.product_line_id = 2
/*left JOIN dbo.phones ph ON  ph.phone_type = 'Phone'
                           AND ph.entity_id = c.entity_id
						   AND ph.entity_type='Provider'
						   AND ISNULL(ph.number,'')&lt;&gt;''
						   AND LEN(ph.number)=10*/

LEFT JOIN StarsConnect.dbo.calls ac ON c.entity_id = ac.entity_id
                                       AND ac.product_line_id = 1
                                       AND ac.call_type_id = 2
                                       AND ac.created_at &gt; c.created_at
                                       AND ac.entity_type = 'Provider'
LEFT JOIN ( SELECT
                MAX(x.created_at) callDate
              , entity_id
            FROM
                StarsConnect.dbo.calls x
            INNER JOIN StarsConnect.dbo.statuses sx ON x.status_id = sx.id
                                                       AND sx.name = 'Complete call'
            GROUP BY
                x.entity_id ) xx ON c.entity_id = xx.entity_id
                                    AND xx.callDate &gt; c.created_at


--AND (CAST(ph.created_at AS DATE)&gt;=CAST(c.created_at AS DATE)
--OR CAST(ISNULL(ph.updated_at,ph.created_at) AS DATE )&gt;=CAST(c.created_at AS DATE))
WHERE
    xx.entity_id IS     NULL
    AND ac.id IS   NULL
    AND c.entity_type = 'Provider'
    AND c.call_type_id IN ( 1 , 2 )
    AND c.product_line_id = 2;</property>
