          <DTS:Variables />
          <DTS:ObjectData>
            <pipeline
              version="1">
              <components>
                <component
                  refId="Package\Populate Resolved DTPS\Stage Billing Resolved\Billing Resolved"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  description="OLE DB Source"
                  name="Billing Resolved"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Declare @PMD_Client_ID int = ?

SELECT DISTINCT
                c.dtpDate openedAt
              , c.rule_identifier
              , c.pmd_patient_id
              , c.pmdCDEId
              , c.cde_dtp_id
              , cdel.pmdLifeCycleStageId
              , CAST(CASE WHEN crd.direction = 'Absence' THEN pcve.evidenceDate
                          WHEN crd.direction = 'Presence' THEN pcve.evidenceExpirationDate
                     END AS DATE) resolvedAt
           
            FROM
                [Stars_Billing].[staging].[MRM_Billing_ClosedDTPs] c
            INNER JOIN [Repl_CREF].[cref_cde].[claimDefinedEventLifecycle] cdel ON c.pmdCDEId = cdel.pmdCDEId
                                                                       AND cdel.pmdLifeCycleStageId IN ( 16 )
                                                                       AND CAST(cdel.created_at AS DATE) BETWEEN c.dtpDate AND closedDate
                                                                            
            INNER JOIN [Repl_CREF].[cref_rulestore].[clientRuleDefinitionsWithUnderlyingVariables] crd WITH ( NOLOCK ) ON c.pmdClinicalRuleID = crd.pmdClinicalRuleId
																													 AND crd.pmdClientId=@PMD_Client_ID      --Added PMD_Client_ID filter so we can run by client      
            INNER JOIN [Repl_CREF].[cref_rulestore].[CVCRResultCheck] cvr ON cvr.pmdClinicalVariableID = crd.pmdClinicalVariableId
                                                                         AND cvr.resultCheckTypeID = 2
            INNER JOIN [Repl_CREF].[cref_pcv].[patientClinicalVariablesEvidence] pcve ON pcve.pmdClinicalVariableId = CASE WHEN crd.underlyingPmdClinicalVariableId = 0
                                                                                                                 THEN crd.pmdClinicalVariableId
                                                                                                                 ELSE crd.underlyingPmdClinicalVariableId
                                                                                                            END
                                                                               AND pcve.pmdPatientId = c.pmd_patient_id
            WHERE
                CASE WHEN crd.direction = 'Absence' THEN pcve.evidenceDate
                     WHEN crd.direction = 'Presence' THEN pcve.evidenceExpirationDate
                END BETWEEN c.dtpDate
                    AND     c.closedDate
                AND CAST(CASE WHEN crd.direction = 'Absence' THEN pcve.evidenceDate
                              WHEN crd.direction = 'Presence' THEN pcve.evidenceExpirationDate
                         END AS DATE) = ( SELECT DISTINCT
                                            CASE WHEN cte2.pmdClinicalVariableId IS NOT  NULL
                                                 THEN MIN(CAST(CASE WHEN crd2.direction = 'Absence' THEN x.evidenceDate
                                                                    WHEN crd2.direction = 'Presence' THEN x.evidenceExpirationDate
                                                               END AS DATE))
                                                 ELSE MAX(CAST(CASE WHEN crd2.direction = 'Absence' THEN x.evidenceDate
                                                                    WHEN crd2.direction = 'Presence' THEN x.evidenceExpirationDate
                                                               END AS DATE))
                                            END
                                          FROM
                                            [Repl_CREF].[cref_pcv].[patientClinicalVariablesEvidence] x
                                          INNER JOIN [Repl_CREF].[cref_rulestore].[clientRuleDefinitionsWithUnderlyingVariables] crd2 ON x.pmdClinicalVariableId = CASE
                                                                                                                                                WHEN crd2.underlyingPmdClinicalVariableId = 0
                                                                                                                                                THEN crd2.pmdClinicalVariableId
                                                                                                                                                ELSE crd2.underlyingPmdClinicalVariableId
                                                                                                                                                END
                                                                                                                                       AND crd2.pmdClinicalRuleId = c.pmdClinicalRuleID
																																	   AND crd2.pmdClientId=@PMD_Client_ID --added filter for PMD_Client_ID
                                          INNER JOIN [Repl_CREF].[cref_rulestore].[CVCRResultCheck] cvr2 ON cvr2.pmdClinicalVariableID = crd2.pmdClinicalVariableId
                                                                                                        AND cvr2.resultCheckTypeID = 2
                                          LEFT JOIN [Stars_Billing].[staging].[MRM_Billing_Variables] cte ON x.pmdClinicalVariableId = cte.pmdClinicalVariableId
                                                                      AND cte.direction = 'Presence'
                                                                      AND cte.cde_dtp_id = c.cde_dtp_id
                                                                      AND cte.pmdClinicalRuleID = c.pmdClinicalRuleID
                                                                      AND cte.pmdClinicalVariableId = CASE WHEN crd2.underlyingPmdClinicalVariableId = 0
                                                                                                           THEN crd2.pmdClinicalVariableId
                                                                                                           ELSE crd2.underlyingPmdClinicalVariableId
                                                                                                      END
                                          LEFT JOIN [Stars_Billing].[staging].[MRM_Billing_Variables] cte2 ON x.pmdClinicalVariableId = cte2.pmdClinicalVariableId
                                                                       AND cte2.direction = 'Presence'
                                                                       AND cte2.cde_dtp_id = c.cde_dtp_id
                                                                       AND cte2.pmdClinicalVariableId &lt;&gt; cte.pmdClinicalVariableId
                                                                       AND cte2.pmdClinicalRuleID = c.pmdClinicalRuleID
                                                                       AND cte2.pmdClinicalVariableId = CASE WHEN crd2.underlyingPmdClinicalVariableId = 0
                                                                                                             THEN crd2.pmdClinicalVariableId
                                                                                                             ELSE crd2.underlyingPmdClinicalVariableId
                                                                                                        END
                                          WHERE 
                                            x.pmdPatientId = c.pmd_patient_id
                                            AND CASE WHEN crd2.direction = 'Absence' THEN x.evidenceDate
