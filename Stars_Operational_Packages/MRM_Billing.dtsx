          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{4CE44E7F-4048-4669-8B7B-76E808B7ED38}"
              SQLTask:SqlStatementSource="----delete the first  intervention if there are different interventions with the same dtpdate and resolveddate &#xA;       &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;    &#xA;     &#xA;            DELETE&#xA;                b&#xA;            FROM&#xA;                [Stars_Billing].[staging].[MRM_Billing_temp_Interventions] b&#xA;            INNER JOIN [Stars_Billing].[staging].[MRM_Billing_Interventions_dupes] d ON b.cdeDTPID = d.cdeDTPID&#xA;            INNER JOIN [Stars_Billing].[staging].[MRM_Billing_temp_Interventions] t ON b.cdeDTPID = t.cdeDTPID&#xA;                                  AND t.dtpDate = b.dtpDate&#xA;                                  AND t.resolvedDate = b.resolvedDate&#xA;                                  AND t.interventionDate &lt;&gt; b.interventionDate&#xA;                                  AND t.interventionTypeID = b.interventionTypeID&#xA;                                  AND t.id &lt;&gt; b.id&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  AND b.billable&lt;&gt;1&#xA;                                  AND b.interventionDate &lt;&gt; ( SELECT&#xA;                                                                MAX(interventionDate)&#xA;                                                              FROM&#xA;                                                                [Stars_Billing].[staging].[MRM_Billing_temp_Interventions] tt&#xA;                                                              WHERE&#xA;                                                                tt.cdeDTPID = b.cdeDTPID&#xA;                                                               /* AND tt.interventionTypeID=b.interventionTypeID*/ )&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;--delete if dp tripped more than twice and resolved more than twice in the same year&#xA;            DELETE&#xA;                b&#xA;            FROM&#xA;                [Stars_Billing].[staging].[MRM_Billing_temp_Interventions] b&#xA;            INNER JOIN [Stars_Billing].[staging].[MRM_Billing_temp_Interventions] t ON b.cdeDTPID = t.cdeDTPID&#xA;            INNER JOIN [Stars_Billing].[staging].[MRM_Billing_Interventions_rank] r ON t.cdeDTPID = r.cdeDTPID&#xA;                                  AND r.rank &gt; 2&#xA;                                  AND t.dtpDate &lt;&gt; b.dtpDate&#xA;                                  AND t.resolvedDate &lt;&gt; b.resolvedDate&#xA;                                  AND t.interventionDate &lt;&gt; b.interventionDate&#xA;                                  AND b.resolvedDate &lt; t.resolvedDate&#xA;                                  AND t.id &lt;&gt; b.id&#xA;                                  AND DATEDIFF(DAY , b.resolvedDate , t.resolvedDate) &lt; 365" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
