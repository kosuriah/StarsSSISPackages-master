                  description="OLE DB Source"
                  name="All Inteventions"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Declare @pmdClientID int = ? 
Declare @startDate date = ?
Declare @endDate date = ?

	--mrm Connect Interventions 
 SELECT --pm.PopulationId, 
                        dtp.pmd_patient_id
                      , dtp.cde_dtp_id
                      , re.pmdCDEId
                      , cast(r.ruleName as varchar(100)) as rule_identifier
                      , dtp.description
                      , CONVERT(VARCHAR(50) , 'Provider Outreach') InterventionType
                      , x.InterventionTypeID
                      , re.dtpDate dtpDate
                      , x.interventionDate InterventionDate
                      , re.resolvedDate resolvedDate
                      , x.NPI
                      , x.Billable
                    FROM  [Repl_CREF].[cref_cde_dtp].[drug_therapy_problems]  dtp
						INNER JOIN [Repl_CREF].[cref_rulestore].[RuleName] r
						ON dtp.parent_id = r.pmdRuleId
						INNER JOIN [Stars_Billing].[staging].[MRM_ResolvedDTPs] re ON dtp.cde_dtp_id = re.cde_dtp_id
                                                      AND re.pmdLifeCycleStageId = 16
                                                      AND re.resolvedDate BETWEEN @startDate AND @endDate
						INNER JOIN [Stars_Billing].[staging].[MRM_Billing_Interventions] x ON dtp.cde_dtp_id = x.CDE_DTP_ID
                                             AND x.InterventionTypeID NOT IN ( 3 , 4 )
                                            
						INNER JOIN [Repl_CREF].[etlcore].[Patient] p ON p.PMD_Patient_ID = dtp.pmd_patient_id
                                                     AND p.PMD_Client_ID IN ( @pmdCLientID )-- 8,43,46,47)
                    WHERE
                        CAST(x.interventionDate AS DATE) BETWEEN re.dtpDate AND re.resolvedDate
                       -- AND x.CDE_DTP_ID=1760375
                        AND DATEDIFF(DAY , CAST(x.interventionDate AS DATE) , re.resolvedDate) &lt;= 365



						UNION 

			 --batch fax interventions
					  SELECT --pm.PopulationId, 
                        dtp.pmd_patient_id
                      , dtp.cde_dtp_id
                      , re.pmdCDEId
                      --, dtp.rule_identifier
					  ,cast( r.ruleName as varchar(100)) as rule_identifier
                      , dtp.description
                      , CONVERT(VARCHAR(50) , 'Batch Fax') InterventionType
                      , x.InterventionTypeID
                      , re.dtpDate dtpDate
                      , x.interventionDate
                      , re.resolvedDate resolvedDate
                      , x.NPI
                      , x.Billable
                    FROM [Repl_CREF].[cref_cde_dtp].[drug_therapy_problems] dtp
					    INNER JOIN [Repl_CREF].[cref_rulestore].[RuleName] r
						ON dtp.parent_id = r.pmdRuleId
						INNER JOIN [Stars_Billing].[staging].[MRM_ResolvedDTPs] re ON dtp.cde_dtp_id = re.cde_dtp_id
                                                AND re.pmdLifeCycleStageId = 16
                                                AND re.resolvedDate BETWEEN @startDate AND @endDate
						INNER JOIN [Stars_Billing].[staging].[MRM_Billing_Interventions] x ON dtp.cde_dtp_id = x.CDE_DTP_ID
                                             AND x.InterventionTypeID = 4

						INNER JOIN [Repl_CREF].[etlcore].[Patient] p ON p.PMD_Patient_ID = dtp.pmd_patient_id
                                                     AND p.PMD_Client_ID IN ( @pmdCLientID )-- 8,43,46,47)
                    WHERE
                        CAST(x.interventionDate AS DATE) BETWEEN re.dtpDate AND re.resolvedDate
                        AND DATEDIFF(DAY , CAST(x.interventionDate AS DATE) , re.resolvedDate) &lt;= 365</property>
