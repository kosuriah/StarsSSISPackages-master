                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Declare @pmdClientID int = ?
Declare @StartDate date = ?
Declare @EndDate date = ?

  SELECT 
                        @pmdCLientID AS PMD_Client_ID
                      , CAST(GETDATE() AS DATE) AS Billdate
                      , t.pmdCdeID
                      , t.cdeDTPID
                      , t.ruleIdentifier AS RuleName
                      , MAX(r.pmdRuleId) AS pmdClinicalRuleId
                      , t.description AS dtpDescription
                      , t.dtpDate
                      , CAST(t.interventionDate AS DATE) AS InterventionDate
                      , t.interventionTypeID AS InterventionTypeID
                      , t.resolvedDate AS ResolutionDate
                      , t.pmdPatientID AS PMD_Patient_ID
                      , p.Patient_ID AS MemberID
                    FROM [Stars_Billing].[staging].[MRM_Billing_temp_Interventions] t
                    INNER JOIN [Repl_CREF].[cref_rulestore].[RuleName] r 
						ON t.ruleIdentifier = r.ruleName
                    INNER JOIN  [Stars_Billing].[staging].[MRM_ResolvedDTPs] re 
						ON re.pmdClinicalRuleId = r.pmdRuleId
                        AND re.cde_dtp_id = t.cdeDTPID
                    INNER JOIN [Repl_CREF].[etlcore].[Patient] p ON p.PMD_Patient_ID = t.pmdPatientID
                                                     AND p.PMD_Client_ID = @pmdCLientID
                    LEFT JOIN  [Stars_Billing].[dbo].[MRM_Billing] db ON t.cdeDTPID = db.cdeDTPID                                                                    
                                                                      AND re.pmdClinicalRuleId = db.pmdClinicalRuleId
                                                                     -- AND db.PMD_Patient_ID = t.pmdPatientID
                                                                      AND db.resolutionDate = t.resolvedDate
                    INNER JOIN (SELECT MAX(interventiondate) AS MaxInterventionDate, cdeDTPID  FROM [Stars_Billing].[staging].[MRM_Billing_temp_Interventions]    GROUP BY cdeDTPID) mi
						ON mi.MaxInterventionDate = t.InterventionDate
						AND mi.cdeDTPID = t.cdeDTPID	
						
					
					
					WHERE
                        db.Billing_ID IS NULL
                        AND re.resolvedDate BETWEEN @startDate AND @endDate
						AND  t.interventionDate &gt;= DATEADD(DAY,-365, re.resolvedDate) --We only bill for intervention dates that are within the 365 day period window prior to the resolution date
                        AND t.billable = 1
                        AND t.interventionDate = ( SELECT
                                                    MAX(i.interventionDate)
                                                   FROM
                                                    [Stars_Billing].[staging].[MRM_Billing_temp_Interventions] i
                                                   WHERE
                                                    i.providerNPI = t.providerNPI
                                                    AND t.cdeDTPID = i.cdeDTPID
                                                    AND t.pmdPatientID = i.pmdPatientID
                        AND i.billable=1)

						

						GROUP BY 
	
                       t.pmdCdeID
                      , t.cdeDTPID
                      , t.ruleIdentifier    
                      , t.description
                      , t.dtpDate
                      , CAST(t.interventionDate AS DATE) 
					  , t.interventionTypeID        
                      , t.resolvedDate 
                      , t.pmdPatientID 
                      , p.Patient_ID</property>
