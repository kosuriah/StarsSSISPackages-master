              SQLTask:SqlStatementSource="DECLARE @MaxRun_ID AS INT = ?&#xA;DECLARE @CurrentDate DATETIME = CAST(CAST(GETDATE() AS DATE) AS DATETIME)&#xA;DECLARE @FirstRecordStartDate DATETIME = CAST(CAST('2013-01-01' AS DATE) AS DATETIME)&#xA;&#xA;&#xA;-- This inserts another record to the dimension for SCD Type 2 changes&#xA;INSERT INTO [Stars_rdsm].[dbo].[Stars_Providers_Detail]&#xA;        ( [Provider_Unique_Ident]&#xA;&#x9;&#x9; ,[Provider_Unique_Key]&#xA;&#x9;&#x9; ,[Address_1]&#xA;&#x9;&#x9; ,[Address_2]&#xA;&#x9;&#x9; ,[City]&#xA;&#x9;&#x9; ,[State]&#xA;&#x9;&#x9; ,[Zip]&#xA;&#x9;&#x9; ,[Phone_Number]&#xA;&#x9;&#x9; ,[Fax_Number]&#xA;         ,[Start_Date] &#xA;         ,[End_Date]          &#xA;         ,[Run_ID]&#xA;        )&#xA;SELECT [Provider_Unique_Ident]&#xA;&#x9;&#x9; ,[Provider_Unique_Key]&#xA;&#x9;&#x9; ,[Address_1]&#xA;&#x9;&#x9; ,[Address_2]&#xA;&#x9;&#x9; ,[City]&#xA;&#x9;&#x9; ,[State]&#xA;&#x9;&#x9; ,[Zip]&#xA;&#x9;&#x9; ,[Phone_Number]&#xA;&#x9;&#x9; ,[Fax_Number]&#xA;         ,[Start_Date] &#xA;         ,[End_Date]    &#xA;         ,@MaxRun_ID&#xA;FROM&#xA;(&#xA;       MERGE [Stars_rdsm].[dbo].[Stars_Providers_Detail] TT &#xA;       USING [Stars_rdsm].[staging].staging_Stars_Providers_Detail_New_Or_Updated ST&#xA;              ON (TT.[Provider_Unique_Ident] = ST.[Provider_Unique_Ident])&#xA;       -- This inserts new records in the dimension table&#xA;       WHEN NOT MATCHED THEN&#xA;              INSERT ([Provider_Unique_Ident]&#xA;&#x9;&#x9; ,[Provider_Unique_Key]&#xA;&#x9;&#x9; ,[Address_1]&#xA;&#x9;&#x9; ,[Address_2]&#xA;&#x9;&#x9; ,[City]&#xA;&#x9;&#x9; ,[State]&#xA;&#x9;&#x9; ,[Zip]&#xA;&#x9;&#x9; ,[Phone_Number]&#xA;&#x9;&#x9; ,[Fax_Number]&#xA;         ,[Start_Date] &#xA;         ,[End_Date]          &#xA;         ,[Run_ID])&#xA;          &#xA;          VALUES ([Provider_Unique_Ident]&#xA;&#x9;&#x9; ,[Provider_Unique_Key]&#xA;&#x9;&#x9; ,[Address_1]&#xA;&#x9;&#x9; ,[Address_2]&#xA;&#x9;&#x9; ,[City]&#xA;&#x9;&#x9; ,[State]&#xA;&#x9;&#x9; ,[Zip]&#xA;&#x9;&#x9; ,[Phone_Number]&#xA;&#x9;&#x9; ,[Fax_Number]&#xA;&#x9;&#x9; ,@FirstRecordStartDate   --Start Date&#xA;&#x9;&#x9; ,NULL  --End Date&#x9;&#x9;&#x9;&#x9; &#xA;&#x9;&#x9; ,@MaxRun_ID) --Run_ID&#xA;       -- This marks the older record to be outdated for SCD Type 2&#xA;       WHEN MATCHED AND TT.[End_Date]  IS NULL THEN&#xA;              UPDATE SET TT.[End_Date] = @CurrentDate&#xA;              OUTPUT $Action Action_Taken,&#xA;&#x9;&#x9;&#x9;&#x9;  ST.[Provider_Unique_Ident] ,&#xA;&#x9;&#x9;&#x9;&#x9;  ST.[Provider_Unique_Key] ,&#xA;&#x9;&#x9;&#x9;&#x9;  ST.[Address_1] ,&#xA;&#x9;&#x9;&#x9;&#x9;  ST.[Address_2] ,&#xA;&#x9;&#x9;&#x9;&#x9;  ST.[City] ,&#xA;&#x9;&#x9;&#x9;&#x9;  ST.[State] ,&#xA;&#x9;&#x9;&#x9;&#x9;  ST.[Zip] ,&#xA;&#x9;&#x9;&#x9;&#x9;  ST.[Phone_Number] ,&#xA;&#x9;&#x9;&#x9;&#x9;  ST.[Fax_Number],&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;  @currentdate AS [Start_Date] ,&#xA;&#x9;&#x9;&#x9;&#x9;  NULL as [End_Date] ,&#xA;&#x9;&#x9;&#x9;&#x9;  @MaxRun_ID AS [Run_ID]&#xA;)&#xA;AS MERGE_OUT&#xA;WHERE MERGE_OUT.Action_Taken = 'UPDATE';&#xA;GO" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
