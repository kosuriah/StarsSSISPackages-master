            <SQLTask:SqlTaskData
              SQLTask:Connection="{833D12C4-85D0-48E0-89ED-50C49F533C60}"
              SQLTask:SqlStatementSource="DECLARE @CurrentDate AS DATETIME = GETDATE()&#xA;&#xA;INSERT INTO staging.StarsConnect_Providers_New&#xA;&#xA;(&#xA;&#x9;[First_Name]&#xA;&#x9;,[Last_Name]&#xA;&#x9;,[Organization_Name]&#xA;&#x9;,[PMD_Provider_ID]&#xA;&#x9;,[NPI]&#xA;&#x9;,[PRIMARY]&#xA;&#x9;,[Imported]&#xA;&#x9;,[Address_1]&#xA;&#x9;,[Address_2]&#xA;&#x9;,[City]&#xA;&#x9;,[State]&#xA;&#x9;,[Zip_Code]&#xA;&#x9;,[verified]&#xA;&#x9;,[Number]&#xA;&#x9;,[Fax_Number]&#xA;&#x9;,[Entity_Type]&#xA;&#x9;,[Created_at]&#xA;&#x9;,[Updated_at]&#xA;)&#xA;&#xA;SELECT  &#xA;&#x9;ISNULL(pr.[First_Name],'')   AS First_Name&#xA;&#x9;,Coalesce(pr.[Last_Name], Organization_Name,'None Provided')   AS Last_Name&#xA;&#x9;,Coalesce(Organization_Name,'None Provided') AS [Organization_Name]&#xA;&#x9;,pr.Provider_Unique_Ident AS PMD_Provider_ID&#xA;&#x9;,pr.Provider_ID AS NPI&#xA;&#x9;,1 AS [PRIMARY]&#xA;&#x9;,1 AS Imported&#xA;&#x9;,ISNULL(CAST(prd.Address_1 AS VARCHAR(250)),'XX') AS Address_1&#xA;&#x9;,ISNULL(CAST(prd.Address_2 AS VARCHAR(250)),'XX')AS Address_2&#xA;&#x9;,ISNULL(CAST(prd.City AS VARCHAR(100)),'XX')AS City&#xA;&#x9;,ISNULL(CAST(prd.State AS VARCHAR(100)),'XX') AS [State]&#xA;&#x9;,ISNULL(CAST(prd.Zip AS VARCHAR(50)),'00000')AS Zip_Code&#xA;&#x9;,0 AS verified&#xA;&#x9;,ISNULL(CAST(prd.Phone_Number AS VARCHAR(15)),'XX') AS Number&#xA;&#x9;,ISNULL(CAST(prd.Fax_Number AS VARCHAR(15)),'XX') AS Fax_Number&#xA;&#x9;,'Provider' AS Entity_Type&#xA;&#x9;,@CurrentDate AS Created_date&#xA;&#x9;,NULL AS Updated_at&#xA;FROM  dbo.Stars_Providers pr WITH (NOLOCK)&#xA;Inner Join dbo.Stars_Providers_Detail prd WITH (NOLOCK) ON pr.Provider_Unique_Ident = prd.Provider_Unique_Ident AND prd.End_Date IS null&#xA;where pr.Provider_Type = 'NPI'&#xA;and not exists &#xA;&#x9;(SELECT 1 FROM [staging].[StarsConnect_Providers_Unique] spu WITH (NOLOCK) where spu.pmd_provider_id = pr.Provider_Unique_Ident)" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
