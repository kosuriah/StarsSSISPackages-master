                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">--SET QUERY_GOVERNOR_COST_LIMIT 0 

--Get complete provider record sets where provider has been updated.

SELECT a.[id]
      ,pr.[first_name]
      ,pr.[last_name]
      ,pr.[pmd_provider_id]
      ,pr.[npi]
      ,pr.[updated_at] AS provider_updated_at
      ,a.address_1
      ,a.address_2
      ,a.city
      ,a.[state]
      ,a.zip_code
      ,a.updated_at AS address_updated_at
      ,Phone_Number.number AS phone_number
	  ,Phone_Number.updated_at AS phone_number_updated_at
      ,Fax_Number.number AS fax_number
	  ,Fax_Number.updated_at AS fax_number_updated_at
      ,'Provider Record Update' AS Update_Reason
  FROM [StarsConnect].[dbo].[providers] pr with (NOLOCK)
 
  INNER JOIN [StarsConnect].[dbo].[addresses] a with (NOLOCK)
  ON pr.id =a.entity_id
  AND a.entity_type = 'Provider'
  AND a.[primary] = 1
   
  INNER JOIN [StarsConnect].[dbo].[phones] AS Phone_Number with (NOLOCK)
  ON Phone_Number.entity_id = pr.id
  AND Phone_Number.phone_type = 'Phone'
  AND Phone_Number.[primary] = 1
  AND Phone_Number.entity_type = 'Provider'

  INNER JOIN [StarsConnect].[dbo].[phones] AS Fax_Number with (NOLOCK)
  ON Fax_Number.entity_id = pr.id
  AND Fax_Number.phone_type = 'Fax'
  AND fax_number.[primary] =1
  AND fax_number.entity_type = 'Provider'

 WHERE DATEDIFF(DAY,pr.updated_at, GETDATE()) &lt; 7

UNION

