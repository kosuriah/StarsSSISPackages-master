      ,Fax_Number.number AS fax_number
	  ,Fax_Number.updated_at AS fax_number_updated_at
      ,'Address Record Update' AS Update_Reason
  FROM [StarsConnect].[dbo].[providers] pr
 
  INNER JOIN [StarsConnect].[dbo].[addresses] a with (NOLOCK)
  ON pr.id =a.entity_id
  AND a.entity_type = 'Provider'
  AND DATEDIFF(DAY,a.updated_at, GETDATE()) &lt; 7 
  AND a.[primary] = 1
  
  INNER JOIN [StarsConnect].[dbo].[phones] AS Phone_Number with (NOLOCK)
