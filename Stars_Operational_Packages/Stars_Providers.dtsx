      ,p.[Provider Business Practice Location Address Postal Code] AS Zip
      ,COALESCE(p.[Provider Business Practice Location Address Telephone Number],p.[Provider Business Mailing Address Telephone Number]) AS Phone_Number
      ,COALESCE(p.[Provider Business Practice Location Address Fax Number],p.[Provider Business Mailing Address Fax Number]) AS Fax_Number

