            <SQLTask:SqlTaskData
              SQLTask:Connection="{833D12C4-85D0-48E0-89ED-50C49F533C60}"
              SQLTask:SqlStatementSource="--Build composite change record for insert into the overrides table&#xA;  INSERT INTO [dbo].[Stars_Providers_Detail_Override]&#xA;  (&#xA;&#x9;[Provider_Id]&#xA;&#x9;,[Address_1]&#xA;&#x9;,[Address_2]&#xA;&#x9;,[City]&#xA;&#x9;,[State]&#xA;&#x9;,[Zip]&#xA;&#x9;,[Phone_Number]&#xA;&#x9;,[Fax_Number]&#xA;&#x9;,[Update_Datetime]&#xA;&#x9;,[Provider_Type]&#xA;  )&#xA;  SELECT &#xA;   m.npi&#xA;  ,ISNULL(LEFT(a.address_1,100),'xx') AS address_1&#xA;  ,ISNULL(LEFT(a.address_2,100),'xx') AS address_2&#xA;  ,ISNULL(LEFT(a.city,100),'xx') AS city&#xA;  ,ISNULL(LEFT(a.[state],40), 'xx') AS [state]&#xA;  ,ISNULL(LEFT(a.zip,20), 'xx') AS zip&#xA;  ,ISNULL(p.phone_number,'xx') AS phone_number&#xA;  ,ISNULL(f.fax_number,'xx') AS fax_number  &#xA;  ,GETDATE() AS Update_Datetime&#xA;  ,'NPI' AS Provider_Type&#xA;  &#xA;  FROM &#xA;  &#xA;  (SELECT distinct&#xA;      [npi]      &#xA;  FROM [STARS_rdsm].[staging].[staging_Stars_Providers_From_StarConnect] sc&#xA;  WHERE NOT EXISTS (SELECT 1 FROM [dbo].[Stars_Providers_Detail_Override] ov WHERE sc.npi = ov.Provider_Id AND ov.Provider_Type = 'NPI')) m  &#xA;  LEFT JOIN staging.Provider_Overrides_Address a&#xA;&#x9;ON a.npi = m.npi&#xA;  LEFT JOIN staging.Provider_Overrides_Phone p&#xA;&#x9;ON p.npi = m.npi&#xA;  LEFT JOIN staging.Provider_Overrides_Fax f&#xA;&#x9;ON f.npi = m.npi" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
