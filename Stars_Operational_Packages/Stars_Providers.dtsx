                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">--Set DNC to 1 
--call level
Select dtp.npi, 1 [DNC_Flag], max(cast(cal.[updated_at] as date)) [DNC_Updated_at]
From [dbo].[dtps] dtp with (nolock)
inner Join (Select max([call_id]) [call_id], [dtp_id] from [dbo].[call_dtps] group by [dtp_id]) maxcall ON maxcall.dtp_id = dtp.id
inner Join [dbo].[calls] cal ON cal.id = maxcall.call_id
inner join [dbo].[measures] m ON m.id = dtp.measure_id
Where dtp.[dtp_Year] = Year(getdate())
and m.[key] in ('HRM') -- Measure HRM
--Do not call again (refused/non-participate)/ Wrong number
and cal.status_detail_id in (1,2,32,33,34,35)
and convert(varchar(10),cal.[updated_at],112) &gt;= convert(varchar(10),getdate()-7,112)
group by dtp.npi

Union

--dtp level
Select dtp.npi, 1 [DNC_Flag], max(cast(dtpdetails.[updated_at] as date)) [DNC_Updated_at]
From [dbo].[dtps] dtp with (nolock)
inner Join (Select max([id]) MaxDTPDetailID, [dtp_id] from dbo.dtp_details WHERE deleted_at IS null group by [dtp_id] ) maxdtpdetail ON maxdtpdetail.dtp_id = dtp.id
Inner Join dbo.dtp_details dtpdetails ON dtpdetails.id = maxdtpdetail.MaxDTPDetailID
inner join [dbo].[measures] m ON m.id = dtp.measure_id
Where dtp.[dtp_Year] = Year(getdate())
and m.[key] in ('HRM') -- Measure HRM
--Specialist
and dtpdetails.sub_category_id in (14)
and convert(varchar(10),dtpdetails.[updated_at],112) &gt;= convert(varchar(10),getdate()-7,112)
and dtpdetails.deleted_at IS NULL GROUP by dtp.npi

Union

--Set DNC to 0 
--call level
Select dtp.npi, 0 [DNC_Flag], max(cast(cal.[updated_at] as date)) [DNC_Updated_at]
From [dbo].[dtps] dtp with (nolock)
inner Join (Select max([call_id]) [call_id], [dtp_id] from [dbo].[call_dtps] group by [dtp_id]) maxcall ON maxcall.dtp_id = dtp.id
inner Join [dbo].[calls] cal ON cal.id = maxcall.call_id
inner join [dbo].[measures] m ON m.id = dtp.measure_id
Where dtp.[dtp_Year] = Year(getdate())
and m.[key] in ('HRM') -- Measure HRM
--Do not call again (refused/non-participate)/ Wrong number
and cal.status_detail_id not in (1,2,32,33,34,35)
and convert(varchar(10),cal.[updated_at],112) &gt;= convert(varchar(10),getdate()-7,112)
group by dtp.npi

Union

--dtp level
Select dtp.npi, 0 [DNC_Flag], max(cast(dtpdetails.[updated_at] as date)) [DNC_Updated_at]
From [dbo].[dtps] dtp with (nolock)
inner Join (Select max([id]) MaxDTPDetailID, [dtp_id] from dbo.dtp_details WHERE deleted_at IS NULL GROUP by [dtp_id]) maxdtpdetail ON maxdtpdetail.dtp_id = dtp.id
Inner Join dbo.dtp_details dtpdetails ON dtpdetails.id = maxdtpdetail.MaxDTPDetailID
inner join [dbo].[measures] m ON m.id = dtp.measure_id
Where dtp.[dtp_Year] = Year(getdate())
and m.[key] in ('HRM') -- Measure HRM
--Specialist
and dtpdetails.sub_category_id not in (14)
and convert(varchar(10),dtpdetails.[updated_at],112) &gt;= convert(varchar(10),getdate()-7,112)
and dtpdetails.deleted_at IS NULL GROUP by dtp.npi</property>
