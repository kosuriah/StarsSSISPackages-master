              SQLTask:SqlStatementSource=" TRUNCATE TABLE staging.Provider_Overrides_Address&#xA; &#xA; INSERT INTO staging.Provider_Overrides_Address (npi, address_1, address_2, city, state, zip)&#xA; &#xA; SELECT npi, address_1, address_2, city, [state], zip_code AS zip    &#xA;  FROM [staging].[staging_Stars_Providers_From_StarConnect] scaddressnew WITH (NOLOCK)&#xA;  WHERE NOT EXISTS (SELECT 1 FROM [dbo].[Stars_Providers_Detail_Override] ov WHERE scaddressnew.npi = ov.Provider_Id AND ov.Provider_Type = 'NPI')&#xA;  AND scaddressnew.update_reason = 'Address Record Update'&#xA;&#xA;&#xA;  TRUNCATE TABLE staging.Provider_Overrides_Phone&#xA;&#xA;  INSERT INTO staging.Provider_Overrides_Phone (npi, phone_number)&#xA;&#xA;  SELECT npi,phone_number    &#xA;  FROM [staging].[staging_Stars_Providers_From_StarConnect] scphonenew WITH (NOLOCK)&#xA;  WHERE NOT EXISTS (SELECT 1 FROM [dbo].[Stars_Providers_Detail_Override] ov WHERE scphonenew.npi = ov.Provider_Id AND ov.Provider_Type = 'NPI')&#xA;  AND scphonenew.update_reason = 'Phone Record Update'&#xA;&#xA;&#xA;&#xA;  TRUNCATE TABLE staging.Provider_Overrides_Fax&#xA;  INSERT INTO staging.Provider_Overrides_Fax (npi, fax_number)&#xA;  SELECT npi,fax_number  &#xA;  FROM [staging].[staging_Stars_Providers_From_StarConnect] scfaxnew WITH (NOLOCK)&#xA;  WHERE NOT EXISTS (SELECT 1 FROM [dbo].[Stars_Providers_Detail_Override] ov WHERE scfaxnew.npi = ov.Provider_Id AND ov.Provider_Type = 'NPI')&#xA;  AND scfaxnew.update_reason = 'Fax Record Update'" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
