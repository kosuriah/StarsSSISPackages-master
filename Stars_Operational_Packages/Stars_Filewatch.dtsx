        public static string FormatSize(Int64 bytes)
        {
            string[] suffixes =  { "Bytes", "KB", "MB", "GB", "TB", "PB" };
            int counter = 0;
            decimal number = (decimal)bytes;
            while (Math.Round(number / 1024,2) >= 1)
            {
                
                number = number / 1024;
                counter++;
                if (number < 1024)
                    break;
                
            }
            return string.Format("{0:n2}{1}", number, suffixes[counter]);
        }
