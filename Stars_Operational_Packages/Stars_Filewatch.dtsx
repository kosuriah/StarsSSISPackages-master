          <DTS:Variables />
          <DTS:ObjectData>
            <SQLTask:SqlTaskData
              SQLTask:Connection="{833D12C4-85D0-48E0-89ED-50C49F533C60}"
              SQLTask:SqlStatementSource="DECLARE @Environment_Flag VARCHAR(10) = ?&#xA;&#xA;MERGE  [STARS_rdsm].[dbo].[Filewatch_History] as target&#xA;&#xA;&#x9;&#x9;&#x9;using(SELECT [PMD_Client_ID]&#xA;&#x9;&#x9;&#x9;&#x9;  ,[Filetype]&#xA;&#x9;&#x9;&#x9;&#x9;  ,[Filename]&#xA;&#x9;&#x9;&#x9;&#x9;  ,CASE WHEN @Environment_Flag = 'Prod' THEN [Filelocation] &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN @Environment_Flag = 'Dev' THEN [FilelocationDEV]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHEN @Environment_Flag = 'QA' THEN [FilelocationQA]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;ELSE 'Dev'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;END AS Filelocation&#xA;&#x9;&#x9;&#x9;&#x9;  ,[Active]&#xA;&#x9;&#x9;&#x9;&#x9;  ,[LastDateReceived]&#xA;&#x9;&#x9;&#x9;&#x9;  ,[Unprocessed_Filecount]&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;  FROM [STARS_rdsm].[dbo].[Filewatch_Config]&#xA;&#x9;&#x9;&#x9;  WHERE [LastDateReceived] IS NOT NULL&#xA;              AND Unprocessed_Filecount &lt;&gt; 0&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;) as source&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;ON (target.PMD_Client_ID=source.PMD_Client_ID&#xA;&#x9;&#x9;&#x9;AND target.filetype=source.Filetype&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;AND CAST(target.LastDateReceived AS DATE)=CAST(source.LastDateReceived AS DATE)&#xA;&#x9;&#x9;&#x9;AND target.unprocessed_filecount = source.Unprocessed_Filecount&#xA;&#x9;&#x9;&#x9;)&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;WHEN matched THEN &#xA;&#x9;&#x9;&#x9;&#x9;update set [LastDateReceived]=source.[LastDateReceived]&#xA;&#xA;&#x9;&#x9;&#x9;when not matched then&#xA;&#x9;&#x9;&#x9;&#x9;Insert ([PMD_Client_ID],[Filetype],[Filename],[Filelocation],[Active],[LastDateReceived],[Unprocessed_Filecount])&#xA;&#x9;&#x9;&#x9;&#x9;values(source.[PMD_Client_ID],source.[Filetype],source.[Filename],source.[Filelocation],source.[Active]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;,source.[LastDateReceived],source.[Unprocessed_Filecount]); " xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
              <SQLTask:ParameterBinding
                SQLTask:ParameterName="0"
                SQLTask:DtsVariableName="$Project::Environment_Flag"
                SQLTask:ParameterDirection="Input"
