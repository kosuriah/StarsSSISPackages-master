            System.IO.FileInfo fileInfo = new System.IO.FileInfo(fullFilePath);
            Dts.Variables["User::File_Row_Count"].Value = Convert.ToInt64(System.IO.File.ReadAllLines(fullFilePath).Length);
            Dts.Variables["User::FileSize"].Value = FormatSize(Convert.ToInt64(fileInfo.Length));
            string actualfilename= fileInfo.Name;
            Dts.Variables["User::Actualfilename"].Value = actualfilename;
